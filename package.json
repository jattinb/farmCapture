{
  "name": "farmcapture",
  "productName": "TrackerBee",
  "version": "1.0.1",
  "description": "Track Wild Pokemon Encounters",
  "main": "dist/bundle.js",
  "scripts": {
    "start": "electron-forge start",
    "start:dev": "electron src/index.js",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "prepare": "mkdir -p temp",
    "build": "webpack",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.app",
    "icon": "resources/masterball_icon",
    "files": [
      "dist/**/*",
      {
        "from": "temp/",
        "to": "temp/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "asarUnpack": [
        "node_modules/screenshot-desktop/lib/win32"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        "zip"
      ],
      "icon": "resources/masterball_icon.ico"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        "deb",
        "rpm"
      ],
      "icon": "resources/masterball_icon.png"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        "zip"
      ]
    }
  },
  "devDependencies": {
    "@babel/cli": "7.24.8",
    "@babel/core": "7.24.9",
    "@babel/preset-env": "7.24.8",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "9.7.0",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "26.0.1",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "babel-loader": "9.1.3",
    "babel-plugin-transform-remove-console": "6.9.4",
    "copy-webpack-plugin": "12.0.2",
    "cssnano": "7.0.4",
    "electron": "31.0.2",
    "electron-builder": "24.13.3",
    "globals": "15.8.0",
    "javascript-obfuscator": "4.1.1",
    "rollup": "2.79.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-html": "0.2.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-obfuscator": "1.1.0",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "speedscope": "1.20.0",
    "webpack": "5.93.0",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0",
    "webpack-obfuscator": "3.5.1"
  },
  "keywords": [],
  "author": {
    "name": "Friendly Neighbourhood Bee"
  },
  "license": "MIT",
  "dependencies": {
    "bootstrap": "5.3.3",
    "csv-parser": "3.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "fuse.js": "7.0.0",
    "jimp": "0.22.12",
    "json2csv": "6.0.0-alpha.2",
    "screenshot-desktop": "^1.15.0",
    "tesseract.js": "5.1.0"
  },
  "engines": {
    "node": "20.x"
  }
}
